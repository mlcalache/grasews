@model Grasews.API.Models.ParseWsdl_ApiResponseViewModel

<div class="modal fade" id="modal-add-model-reference-from-ontology" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title">@WebResource.AddModelReference</h4>
            </div>
            <div class="modal-body">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>@WebResource.ServiceDescriptionElements</label>
                                <select id="ddl-add-modal-reference-wsdl-element" class="form-control select2 select2-hidden-accessible" style="width: 100%;" tabindex="-1" aria-hidden="true">
                                    @Html.Partial("~/Views/Site/_PartialViews/_ServiceDescriptionElementsSelectOptions.cshtml", Model)
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default pull-left" data-dismiss="modal">@WebResource.Close</button>
                <button type="button" class="btn btn-primary" id="lnkApplyModelReferenceFromOntology" data-dismiss="modal">@WebResource.Apply</button>
            </div>
        </div>
    </div>
</div>