]>


<rdf:RDF xmlns="http://dcm.ffclrp.usp.br/lssb/geas/ontologies/GEXPO.owl#"
     xml:base="http://dcm.ffclrp.usp.br/lssb/geas/ontologies/GEXPO.owl"
     xmlns:ro="http://www.obofoundry.org/ro/ro.owl#"
     xmlns:GeneExpressionOntology="http://dcm.ffclrp.usp.br/lssb/geas/ontologies/GEXPO.owl#"
     
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:SO="http://purl.org/obo/owl/SO#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:GO="http://purl.org/obo/owl/GO#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="http://dcm.ffclrp.usp.br/lssb/geas/ontologies/GEXPO.owl"/>
    


