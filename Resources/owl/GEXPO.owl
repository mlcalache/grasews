     -->

    <owl:AnnotationProperty rdf:about="&GeneExpressionOntology;definition"/>
    <owl:AnnotationProperty rdf:about="&GeneExpressionOntology;synonym"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


