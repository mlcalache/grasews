    <!-- http://purl.org/obo/owl/GO#GO_0001171 -->

    <owl:Class rdf:about="&GO;GO_0001171">
        <rdfs:label rdf:datatype="&xsd;string">reverse transcription</rdfs:label>
        <rdfs:subClassOf rdf:resource="&GO;GO_0008150"/>
        <rdfs:subClassOf>
