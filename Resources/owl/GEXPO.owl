    <!-- http://dcm.ffclrp.usp.br/lssb/geas/ontologies/GEXPO.owl#Applied_Biosystems_SOLiD -->

    <owl:NamedIndividual rdf:about="&GeneExpressionOntology;Applied_Biosystems_SOLiD">
        <rdf:type rdf:resource="&GeneExpressionOntology;GEXPO_0000062"/>
        <rdfs:label rdf:datatype="&xsd;string">Applied Biosystems SOLiD</rdfs:label>
        <definition rdf:datatype="&xsd;string">Applied Biosystems SOLiDis a massively parallel next-generation sequencing platform that enables highly accurate sequencing and support a wide range of research applications such as exome sequencing and RNA-Seq on a pay-per-lane basis. It offers platform-optimized sample preparation, individually configurable sequencing lanes, superior sequencing chemistry, and biologist-friendly data analysis solutions.</definition>
